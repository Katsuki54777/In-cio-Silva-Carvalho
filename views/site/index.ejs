<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>StreamFlix - Sua Plataforma de Streaming</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
            background-color: #000;
            color: #fff;
            line-height: 1.6;
        }

        /* Header */
        header {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            z-index: 1000;
            background: linear-gradient(to bottom, rgba(0,0,0,1), rgba(0,0,0,0));
            padding: 20px 60px;
        }

        .header-container {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .logo {
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .logo-icon {
            width: 32px;
            height: 32px;
            color: #e50914;
        }

        .logo-text {
            font-size: 24px;
            font-weight: bold;
            color: #e50914;
            letter-spacing: 2px;
        }

        nav {
            display: flex;
            gap: 30px;
        }

        nav a {
            color: #fff;
            text-decoration: none;
            transition: color 0.3s;
        }

        nav a:hover {
            color: #b3b3b3;
        }

        .user-actions {
            display: flex;
            align-items: center;
            gap: 20px;
        }

        .user-avatar {
            width: 32px;
            height: 32px;
            background-color: #e50914;
            border-radius: 4px;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        /* Hero Section */
        .hero {
            position: relative;
            height: 80vh;
            display: flex;
            align-items: center;
            padding: 0 60px;
            margin-top: 0;
        }

        .hero-background {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            overflow: hidden;
        }

        .hero-background img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .hero-overlay {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(to top, #000 0%, rgba(0,0,0,0.5) 50%, transparent 100%),
                        linear-gradient(to right, rgba(0,0,0,0.8) 0%, transparent 100%);
        }

        .hero-content {
            position: relative;
            z-index: 10;
            max-width: 600px;
        }

        .hero-badge {
            display: inline-block;
            background-color: #e50914;
            padding: 8px 16px;
            border-radius: 4px;
            font-size: 12px;
            font-weight: bold;
            margin-bottom: 20px;
        }

        .hero-content h1 {
            font-size: 48px;
            margin-bottom: 20px;
            line-height: 1.2;
        }

        .hero-content p {
            font-size: 18px;
            color: #b3b3b3;
            margin-bottom: 30px;
            line-height: 1.6;
        }

        .hero-buttons {
            display: flex;
            gap: 15px;
            margin-bottom: 30px;
        }

        .btn {
            padding: 15px 35px;
            border: none;
            border-radius: 4px;
            font-size: 16px;
            font-weight: bold;
            cursor: pointer;
            display: flex;
            align-items: center;
            gap: 10px;
            transition: all 0.3s;
        }

        .btn-primary {
            background-color: #fff;
            color: #000;
        }

        .btn-primary:hover {
            background-color: #e5e5e5;
        }

        .btn-secondary {
            background-color: rgba(109, 109, 110, 0.7);
            color: #fff;
        }

        .btn-secondary:hover {
            background-color: rgba(109, 109, 110, 0.9);
        }

        .hero-meta {
            display: flex;
            gap: 20px;
            color: #b3b3b3;
            font-size: 14px;
        }

        .rating-badge {
            border: 1px solid #808080;
            padding: 4px 8px;
        }

        /* Main Content */
        main {
            padding: 40px 60px;
        }

        section {
            margin-bottom: 60px;
        }

        h2 {
            font-size: 28px;
            margin-bottom: 30px;
        }

        h3 {
            font-size: 16px;
            margin-bottom: 8px;
        }

        h4 {
            font-size: 18px;
            margin-bottom: 15px;
        }

        /* Movie Grid */
        .movie-grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
            gap: 20px;
        }

        .movie-card {
            cursor: pointer;
            transition: transform 0.3s;
        }

        .movie-card:hover {
            transform: scale(1.05);
        }

        .movie-poster {
            position: relative;
            aspect-ratio: 2/3;
            overflow: hidden;
            border-radius: 8px;
            margin-bottom: 10px;
        }

        .movie-poster img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .movie-overlay {
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
            padding: 20px;
            background: linear-gradient(to top, rgba(0,0,0,0.8), transparent);
            opacity: 0;
            transition: opacity 0.3s;
        }

        .movie-card:hover .movie-overlay {
            opacity: 1;
        }

        .movie-rating {
            display: flex;
            align-items: center;
            gap: 5px;
            color: #fbbf24;
            font-size: 14px;
        }

        .movie-info {
            display: flex;
            gap: 10px;
            color: #9ca3af;
            font-size: 14px;
        }

        /* Pricing Plans */
        .pricing-section {
            padding: 80px 0;
        }

        .pricing-header {
            text-align: center;
            margin-bottom: 60px;
        }

        .pricing-header p {
            color: #9ca3af;
            max-width: 600px;
            margin: 0 auto;
            margin-top: 15px;
        }

        .pricing-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 30px;
            max-width: 1200px;
            margin: 0 auto;
        }

        .plan-card {
            background-color: rgba(17, 24, 39, 0.5);
            border: 2px solid #4b5563;
            border-radius: 12px;
            padding: 40px 30px;
            position: relative;
            transition: transform 0.3s;
        }

        .plan-card:hover {
            transform: scale(1.05);
        }

        .plan-card.popular {
            border-color: #e50914;
        }

        .popular-badge {
            position: absolute;
            top: -15px;
            left: 50%;
            transform: translateX(-50%);
            background-color: #e50914;
            padding: 8px 20px;
            border-radius: 20px;
            font-size: 12px;
            font-weight: bold;
        }

        .plan-header {
            text-align: center;
            margin-bottom: 30px;
        }

        .plan-name {
            font-size: 24px;
            margin-bottom: 10px;
        }

        .plan-price {
            font-size: 32px;
            color: #fff;
        }

        .plan-period {
            font-size: 16px;
            color: #9ca3af;
        }

        .plan-features {
            list-style: none;
            margin-bottom: 40px;
        }

        .plan-features li {
            padding: 12px 0;
            display: flex;
            align-items: flex-start;
            gap: 10px;
            color: #d1d5db;
        }

        .check-icon {
            color: #10b981;
            flex-shrink: 0;
            margin-top: 2px;
        }

        .plan-button {
            width: 100%;
            padding: 15px;
            border: none;
            border-radius: 6px;
            font-size: 16px;
            font-weight: bold;
            cursor: pointer;
            transition: background-color 0.3s;
        }

        .plan-button.primary {
            background-color: #e50914;
            color: #fff;
        }

        .plan-button.primary:hover {
            background-color: #b20710;
        }

        .plan-button.secondary {
            background-color: #374151;
            color: #fff;
        }

        .plan-button.secondary:hover {
            background-color: #4b5563;
        }

        /* Footer */
        footer {
            background-color: rgba(0,0,0,0.5);
            border-top: 1px solid #1f2937;
            padding: 60px 60px 30px;
            margin-top: 80px;
        }

        .footer-content {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 40px;
            margin-bottom: 40px;
        }

        .footer-column h4 {
            margin-bottom: 20px;
        }

        .footer-column p {
            color: #9ca3af;
            line-height: 1.8;
        }

        .footer-links {
            list-style: none;
        }

        .footer-links li {
            margin-bottom: 10px;
        }

        .footer-links a {
            color: #9ca3af;
            text-decoration: none;
            transition: color 0.3s;
        }

        .footer-links a:hover {
            color: #fff;
        }

        .social-icons {
            display: flex;
            gap: 15px;
        }

        .social-icon {
            width: 40px;
            height: 40px;
            background-color: #1f2937;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            transition: background-color 0.3s;
        }

        .social-icon:hover {
            background-color: #e50914;
        }

        .footer-bottom {
            border-top: 1px solid #1f2937;
            padding-top: 30px;
            text-align: center;
            color: #6b7280;
        }

        /* Responsive */
        @media (max-width: 768px) {
            header {
                padding: 15px 20px;
            }

            nav {
                display: none;
            }

            .hero {
                padding: 0 20px;
                height: 70vh;
            }

            .hero-content h1 {
                font-size: 32px;
            }

            main {
                padding: 20px;
            }

            .movie-grid {
                grid-template-columns: repeat(2, 1fr);
            }

            footer {
                padding: 40px 20px 20px;
            }
        }
    </style>
</head>
<body>
    <!-- Header -->
    <header>
        <div class="header-container">
            <div class="logo">
                <svg class="logo-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                    <rect x="2" y="2" width="20" height="20" rx="2.18" ry="2.18"></rect>
                    <line x1="7" y1="2" x2="7" y2="22"></line>
                    <line x1="17" y1="2" x2="17" y2="22"></line>
                    <line x1="2" y1="12" x2="22" y2="12"></line>
                    <line x1="2" y1="7" x2="7" y2="7"></line>
                    <line x1="2" y1="17" x2="7" y2="17"></line>
                    <line x1="17" y1="17" x2="22" y2="17"></line>
                    <line x1="17" y1="7" x2="22" y2="7"></line>
                </svg>
                <span class="logo-text">STREAMFLIX</span>
            </div>
            
            <nav>
                <a href="#inicio">Início</a>
                <a href="#filmes">Filmes</a>
                <a href="#series">Séries</a>
                <a href="#planos">Planos</a>
            </nav>
            
            <div class="user-actions">
                <div class="user-avatar">
                    <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path>
                        <circle cx="12" cy="7" r="4"></circle>
                    </svg>
                </div>
                <span>João Silva</span>
            </div>
        </div>
    </header>

    <!-- Hero Section -->
    <section class="hero" id="inicio">
        <div class="hero-background">
            <img src="https://images.unsplash.com/photo-1762356121454-877acbd554bb?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxhY3Rpb24lMjBtb3ZpZSUyMHBvc3RlcnxlbnwxfHx8fDE3NjMwMDQwOTd8MA&ixlib=rb-4.1.0&q=80&w=1080" alt="Hero">
            <div class="hero-overlay"></div>
        </div>
        
        <div class="hero-content">
            <div class="hero-badge">FILME EM DESTAQUE</div>
            <h1>Missão Impossível: Protocolo Fantasma</h1>
            <p>
                Um agente especial é acusado de um ataque terrorista ao Kremlin e precisa provar sua inocência 
                enquanto tenta impedir um plano que pode levar o mundo à guerra nuclear. Uma emocionante 
                jornada repleta de ação e suspense.
            </p>
            
            <div class="hero-buttons">
                <button class="btn btn-primary">
                    <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
                        <polygon points="5 3 19 12 5 21 5 3"></polygon>
                    </svg>
                    Assistir
                </button>
                <button class="btn btn-secondary">
                    <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <circle cx="12" cy="12" r="10"></circle>
                        <line x1="12" y1="16" x2="12" y2="12"></line>
                        <line x1="12" y1="8" x2="12.01" y2="8"></line>
                    </svg>
                    Mais Informações
                </button>
            </div>
            
            <div class="hero-meta">
                <span class="rating-badge">16+</span>
                <span>2h 13min</span>
                <span>Ação, Suspense</span>
            </div>
        </div>
    </section>

    <!-- Main Content -->
    <main>
        <!-- Filmes em Destaque -->
        <section id="filmes">
            <h2>Filmes em Destaque</h2>
            <div class="movie-grid">
                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="https://images.unsplash.com/photo-1762356121454-877acbd554bb?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxhY3Rpb24lMjBtb3ZpZSUyMHBvc3RlcnxlbnwxfHx8fDE3NjMwMDQwOTd8MA&ixlib=rb-4.1.0&q=80&w=1080" alt="Ação Extrema">
                        <div class="movie-overlay">
                            <div class="movie-rating">
                                <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
                                    <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                                </svg>
                                4.5
                            </div>
                        </div>
                    </div>
                    <h3>Ação Extrema</h3>
                    <div class="movie-info">
                        <span>2023</span>
                        <span>•</span>
                        <span>Ação</span>
                    </div>
                </div>

                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="https://images.unsplash.com/photo-1590562177087-ca6af9bb82ea?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxzY2ktZmklMjBtb3ZpZXxlbnwxfHx8fDE3NjI5OTQ4MzN8MA&ixlib=rb-4.1.0&q=80&w=1080" alt="Viagem Intergaláctica">
                        <div class="movie-overlay">
                            <div class="movie-rating">
                                <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
                                    <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                                </svg>
                                4.8
                            </div>
                        </div>
                    </div>
                    <h3>Viagem Intergaláctica</h3>
                    <div class="movie-info">
                        <span>2024</span>
                        <span>•</span>
                        <span>Ficção Científica</span>
                    </div>
                </div>

                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="https://images.unsplash.com/photo-1664249558732-7a779ec1321a?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxkcmFtYSUyMGZpbG18ZW58MXx8fHwxNjMwMDEyfDA&ixlib=rb-4.1.0&q=80&w=1080" alt="Drama Urbano">
                        <div class="movie-overlay">
                            <div class="movie-rating">
                                <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
                                    <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                                </svg>
                                4.3
                            </div>
                        </div>
                    </div>
                    <h3>Drama Urbano</h3>
                    <div class="movie-info">
                        <span>2023</span>
                        <span>•</span>
                        <span>Drama</span>
                    </div>
                </div>

                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="https://images.unsplash.com/photo-1758525862250-5d1f64dc140b?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHx0aHJpbGxlciUyMG1vdmllfGVufDF8fHx8MTc2Mjk2MDQ5Nnww&ixlib=rb-4.1.0&q=80&w=1080" alt="O Segredo Oculto">
                        <div class="movie-overlay">
                            <div class="movie-rating">
                                <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
                                    <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                                </svg>
                                4.6
                            </div>
                        </div>
                    </div>
                    <h3>O Segredo Oculto</h3>
                    <div class="movie-info">
                        <span>2024</span>
                        <span>•</span>
                        <span>Suspense</span>
                    </div>
                </div>
                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="<% filmes.forEach(i => { %><%=i.foto%><% }) %>" alt="Homem Aranha">
                        <div class="movie-overlay">
                            <div class="movie-rating">
                                <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
                                    <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                                </svg>
                                4.3
                            </div>
                        </div>
                    </div>
                 <h3>Homem Aranha</h3>
                    <div class="movie-info">
                      <span>2003</span>
                      <span>•</span>
                      <span>Ficção Científica</span>
                        <% filmes.forEach(i => { %>
                      <tr>
                      <td><%= i.nome %></td>
                      <td><%= i.duracao %></td>
                      <td><%= i.ano %></td>
                      <td><%= i.dublagem %></td>
                     
                     </tr>
                     <% }) %>
         
                    </div>
            </div>
            
         
        
        </section>

        <!-- Séries Populares -->
        <section id="series">
            <h2>Séries Populares</h2>
            <div class="movie-grid">
                <div class="movie-card">
                    <div class="movie-poster">
                        <img src="https://images.unsplash.com/photo-1678529262996-a75ab8d146b7?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=M3w3Nzg4Nzd8MHwxfHNlYXJjaHwxfHxteXN0ZXJ5JTIwc2hvd3xlbnwxfHx8fDE3NjMwNTg5ODB8MA&ixlib=rb-4.1.0&q=80&w=1080" alt="Mistérios da Noite">
