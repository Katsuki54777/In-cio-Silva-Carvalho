     <%-include('../cabecalho.ejs')%>
     <header>
        <h3>Adicionar Novo Time</h3>
      </header>
      <div class="content">
        <form id="form-times" autocomplete="off" action="/times/addok" method="POST">
          <div class="grid">
            <div>
              <label for="nome">Nome do time</label>
              <input id="nome" name="nome" type="text" placeholder="Ex: Minas Tênis Clube" required>
            </div>
            <div>
              <label for="time">Time</label>
              <select id="marca" name="marca" required>
                <option value="" disabled selected>Selecione o Time</option>
                <option>Sada Cruzeiro Vôlei</option>
                <option>Sesi Vôlei Bauru</option>
                <option>Praia Clube</option>
              </select>
            </div>
            <div>
              <label for="cidade">Cidade</label>
              <select id="cidade" name="cidade" required>
                <option value="" disabled selected>Selecione o tipo</option>
                <option>Minas Gerais</option>
                <option>São Paulo</option>
                <option>Uberlândia</option>
              </select>
            </div>
          <footer>
            <a type="button" class="btn ghost" id="btn-cancelar" href="/times/lst">Cancelar</a>
            <button type="submit" class="btn primary">Adicionar Time</button>
          </footer>
        </form>
      </div>

      <%-include('../rodape.ejs')%>